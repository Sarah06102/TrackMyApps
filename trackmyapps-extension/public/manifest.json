{
    "manifest_version": 3,
    "name": "TrackMyApps",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png"
        }

    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"], 
        "js": ["content.js"]
        }
  ]
}