{
    "manifest_version": 3,
    "name": "TrackMyApps",
    "version": "1.0",
    "oauth2": {
        "client_id": "693498453789-o4h4ere1bksc1m17k7vsa20mkml9i6qj.apps.googleusercontent.com",
        "scopes": ["email", "profile", "openid"]
},
    "permissions": [
        "identity",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png"
        }

    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"], 
        "js": ["content.js"]
        }
    
  ]
}